import sqlite3 as lite

conn = lite.connect('d:/embers/financemodel/embers_v2.db')
cur = conn.cursor()
sql = "select country,date,holiday_name from s_holiday"
cur.execute(sql)
rs = cur.fetchall()
holidays = {}
for r in rs:
    country = r[0]
    if country not in holidays:
        holidays[country] = {}
    post_date = r[1]
    holiday_name = r[2]
    holidays[country][post_date] = holiday_name
    


if conn:
    conn.close()