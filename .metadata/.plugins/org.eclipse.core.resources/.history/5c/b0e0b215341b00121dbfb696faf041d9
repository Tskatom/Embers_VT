import json
import numpy
import nltk
import sys
from Util import calculator
import sqlite3 as lite



con = None
try:
    con = lite.connect("d:/sqlite/embers.db")
    cur = con.cursor()
    cur.execute('select sqlite_version()')
    data = cur.fetchone()
    print "Sqlite Version: %s" % data
    
except lite.Error, e:
    print "Error %s:" % e.args[0]
    sys.exit(1)
finally:
    if con:
        con.close()    