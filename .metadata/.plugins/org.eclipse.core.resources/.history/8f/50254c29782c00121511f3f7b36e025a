import json

def analysis():
    bd_list = json.load(open('d:/embers/bd_relation_results.json','r'))
    for (k,v) in bd_list.items():
        print k,len(v)
        for item in v:
            print item["post_date"], ":" len(item('related_events'))
        

if __name__ == "__main__":
    analysis()
        