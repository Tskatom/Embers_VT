import matplotlib.pyplot as plt
import numpy as np
from Util import common
import os


def main():
    #get stock index values
    cfgPath = "../Config/config.cfg"
    common.init(cfgPath)
    
    con = common.getDBConnection()
    cur = con.cursor()
    sql = "select stock_index,date,zscore30,zscore90 from t_daily_stock order by stock_index,date"
    cur.execute(sql)
    rows = cur.fetchall()
    stockBasket = {}
    for row in rows:
        stockIndex = row[0]
        date = row[1]
        zscore30 = row[2]
        zscore90 = row[3]
        
if __name__ == "__main__":
    main()            